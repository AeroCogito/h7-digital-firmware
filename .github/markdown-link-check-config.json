{
  "ignorePatterns": [
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^https://github.com/.*/releases/tag/.*-signed$"
    },
    {
      "pattern": "^\\.\\./\\.\\./releases"
    },
    {
      "pattern": "^\\.\\./\\.\\./actions"
    },
    {
      "pattern": "^https://www\\.st\\.com"
    }
  ],
  "replacementPatterns": [],
  "httpHeaders": [
    {
      "urls": ["https://github.com"],
      "headers": {
        "Accept-Encoding": "zstd, br, gzip, deflate"
      }
    }
  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 206, 299]
}
